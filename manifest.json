{
  "manifest_version": 3,
  "name": "TextLens",
  "version": "1.0",
  "description": "Tools to simplify the work",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "css": ["content_styles.css"],
      "all_frames": true
    }
  ],
  "permissions": ["activeTab"]
}
